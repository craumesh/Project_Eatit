<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.eatit.mapper.machineMapper">
  
  <!-- 설비 추가  -->
  <insert id="insertmachine">
  insert into machine(machine_code,machine_name,member_no,installation_date=now(),machine_location)
  values(#{machine_code},#{machine_name},#{member_no},#{machine_location})
  </insert>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  </mapper>