<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.eatit.mapper.MasterDataMapper">
  
   <select id="getProductList" resultType="ProductVO">
    	select * from product_information 
    </select>
  
  <insert id="insertProduct">
    insert into product_information(product_code,product_name,product_category,company_no,product_unit,product_price,recipe)
    values(#{product_code},#{product_name},#{product_category},#{company_no},#{product_unit},#{product_price},#{recipe})
  </insert>
  
  
  
  </mapper>