<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.eatit.mapper.productionMapper">
  

  <!-- 설비 페이징 처리 갯수 -->
  <select id="machinetotalCount" resultType="int">
  		<![CDATA[
	  		SELECT COUNT(*)
	  		FROM machine
	  		WHERE machine_code > 0
  		]]>
  </select>

  
  
  
  
  
  
